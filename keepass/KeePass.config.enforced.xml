<?xml version="1.0" encoding="utf-8"?>
<Configuration xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Application>
		<TriggerSystem>
			<Triggers>
			</Triggers>
		</TriggerSystem>
	</Application>
    <UI>
		<StandardFont>
			<Family>Maple Mono NF CN</Family>
			<Size>10.2857132</Size>
			<GraphicsUnit>Point</GraphicsUnit>
			<OverrideUIDefault>true</OverrideUIDefault>
		</StandardFont>
    </UI>
	<PasswordGenerator>
		<ProfilesEnabled>true</ProfilesEnabled>
		<AutoGeneratedPasswordsProfile>
			<GeneratorType>CharSet</GeneratorType>
			<Length>20</Length>
			<CharSetRanges>ULD_______</CharSetRanges>
		</AutoGeneratedPasswordsProfile>
		<UserProfiles>
			<Profile>
				<Name>ULC-DGT-8</Name>
				<GeneratorType>Pattern</GeneratorType>
				<Length>20</Length>
				<CharSetRanges>ULD_______</CharSetRanges>
				<Pattern>ludA{5}</Pattern>
				<PatternPermutePassword>true</PatternPermutePassword>
				<ExcludeLookAlike>true</ExcludeLookAlike>
			</Profile>
			<Profile>
				<Name>UPC-DGT-MINUS-8</Name>
				<GeneratorType>Pattern</GeneratorType>
				<Length>8</Length>
				<CharSetRanges>U_D__m____</CharSetRanges>
				<Pattern>\-udU{5}</Pattern>
				<PatternPermutePassword>true</PatternPermutePassword>
				<ExcludeLookAlike>true</ExcludeLookAlike>
			</Profile>
		</UserProfiles>
	</PasswordGenerator>
    <Custom>
		<Item>
			<Key>AutoTypeSearchExt.DefaultAction</Key>
			<Value>PerformAutoType</Value>
		</Item>
		<Item>
			<Key>AutoTypeSearchExt.ShowHotKey</Key>
			<Value>Ctrl+Alt+E</Value>
		</Item>
		<Item>
			<Key>AutoTypeSearchExt.AlternativeAction</Key>
			<Value>PerformAutoTypePassword</Value>
		</Item>
		<Item>
			<Key>AutoTypeSearchExt.WindowPosition</Key>
			<Value>0, 0, 0, 0</Value>
		</Item>
    </Custom>
</Configuration>